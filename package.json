{
  "name": "api-test",
  "version": "1.0.0",
  "description": "Automate Rest API testing",
  "main": "index.js",
  "dependencies": {
    "chai": "^4.2.0",
    "chai-json-schema": "^1.5.1",
    "dotenv": "^8.2.0",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "mocha": "^11.5.0",
    "mochawesome": "^7.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/_mocha ./api/test --recursive --reporter mochawesome --reporter-options reportDir=reports/mochawesome/ --timeout 180000",
    "reports": "xdg-open 'reports/mochawesome/mochawesome.html'"
  },
  "repository": {
